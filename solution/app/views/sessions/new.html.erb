<h2>Sign In</h2>

<form action="<%= session_url %>" method="post">
  <%= authenticity_token %>

  <label>
    Username
    <input type="text" name="wizard[username]" value="">
  </label>

  <label>
    Password
    <input type="password" name="wizard[password]" value="">
  </label>

  <input type="submit" value="Sign In">
</form>
